                <Grid Background="#FFE5E5E5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>
                    <WrapPanel Grid.Row="0" HorizontalAlignment="Right">
                        <Button
                            x:Name="btnVerificationTG"
                            Margin="10,0,10,0"
                            Padding="10"
                            Click="btnVerificationTG_Click"
                            Content="照査実行" />
                    </WrapPanel>
                    <TabControl x:Name="TGTabControl" Grid.Row="1">
                        <TabItem Header="諸条件の設定">
                            <StackPanel>
                                <VerificationItem:TGConditions
                                    x:Name="TGCon"
                                    ButtonVisibility="Collapsed"
                                    IsEditable="False" />
                            </StackPanel>
                        </TabItem>
                        <TabItem Header="照査結果">
                            <Main:TGVRTabs x:Name="TGVR" />
                        </TabItem>
                    </TabControl>
