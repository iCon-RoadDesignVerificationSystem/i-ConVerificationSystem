        </TabControl>
        <ToolBarTray
            Grid.Row="2"
            Height="20"
            VerticalAlignment="Bottom"
            IsLocked="True">
            <ToolBar>
                <ToolBarPanel Orientation="Horizontal">
                    <TextBlock x:Name="IsStdConditionsJsonLoaded" />
                </ToolBarPanel>
                <ToolBarPanel Margin="10,0,0,0" Orientation="Horizontal">
                    <TextBlock Text="設定ファイル読込：" />
                    <TextBlock x:Name="IsConditionsJsonLoaded" />
                    <TextBlock
                        Margin="5,0,0,0"
                        Foreground="DarkGreen"
                        Text="{Binding Model.ConditionsJsonFilePath.Value}" />
                </ToolBarPanel>
                <ToolBarPanel Margin="10,0,0,0" Orientation="Horizontal">
                    <TextBlock HorizontalAlignment="Right" Text="最終変更日時：" />
                    <TextBlock HorizontalAlignment="Right" Text="{Binding Model.ConditionsUpdatedTime.Value, StringFormat=yyyy年MM月dd日 HH:mm:ss}" />
                </ToolBarPanel>
                <ToolBarPanel Margin="10,0,0,0" Orientation="Horizontal">
                    <TextBlock HorizontalAlignment="Right" Text="最終保存日時：" />
                    <TextBlock HorizontalAlignment="Right" Text="{Binding Model.ConditionsSavedTime.Value, StringFormat=yyyy年MM月dd日 HH:mm:ss}" />
                </ToolBarPanel>
            </ToolBar>
        </ToolBarTray>
    </Grid>
</Window>
