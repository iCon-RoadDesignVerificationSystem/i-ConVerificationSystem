                    <Grid.RowDefinitions>
                        <RowDefinition Height="40" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>
                    <WrapPanel Grid.Row="0" HorizontalAlignment="Right">
                        <Button
                            x:Name="btnVerificationOG"
                            Margin="10,0,10,0"
                            Padding="10"
                            Click="btnVerificationOG_Click"
                            Content="照査実行" />
                    </WrapPanel>
                    <TabControl x:Name="OGTabControl" Grid.Row="1">
